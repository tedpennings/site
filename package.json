{
  "name": "site",
  "version": "0.2.0",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.11.6",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "@mdx-js/loader": "^2.0.0-next.7",
    "@svgr/webpack": "5.4.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "camelcase": "^6.0.0",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "concurrently": "^5.3.0",
    "css-loader": "^4.3.0",
    "cypress": "^5.1.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-mdx": "^1.8.2",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "file-loader": "6.1.0",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "4.4.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.4.2",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "26.4.0",
    "jest-watch-typeahead": "0.6.1",
    "jss": "^10.0.3",
    "medium-zoom": "^1.0.6",
    "mini-css-extract-plugin": "0.10.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "9.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.2",
    "prettier": "2.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "resolve": "1.17.0",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "10.0.2",
    "semver": "7.3.2",
    "style-loader": "1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "url-loader": "4.1.0",
    "use-react-router-breadcrumbs": "^1.0.4",
    "wavesurfer.js": "^4.0.1",
    "webpack": "4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-dev-server": "3.11.0",
    "webpack-manifest-plugin": "2.2.0"
  },
  "scripts": {
    "lint:eslint": "eslint src cypress config scripts --ext js,jsx,json,mdx,md",
    "lint:prettier": "prettier --check src cypress config scripts",
    "lint": "concurrently -n 'prettier,eslint' yarn:lint:prettier yarn:lint:eslint",
    "start": "node scripts/start.js",
    "open:cypress": "NODE_ENV=test yarn cypress open --config baseUrl=http://localhost:3000/",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(jsx?)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(jsx?|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(jsx?)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
